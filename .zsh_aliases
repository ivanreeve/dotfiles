alias nvim=~/.local/bin/./nvim.appimage

# TMUX
alias t="tmux"
alias tl="tmux ls"
alias ta="tmux attach -t"
alias tk="tmux kill-session -t"
alias tka="tmux kill-server"
alias tn="tmux new -s"

# GIT
alias ga="git add"
alias gc="git commit -m"
alias gp="git push origin"
alias gs="git status"

gcl(){
  git clone https://${GH_KEY}@github.com/$1 ${@:2}
}

# BASH
alias ls="ls -alh"
alias startpg='~/pgadmin4/pgadmin4env/bin/python ~/pgadmin4/pgadmin4env/lib/python3.12/site-packages/pgadmin4/pgAdmin4.py'
